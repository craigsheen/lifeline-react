{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["DOM", "ES2015", "ES2017"],
    "module": "commonjs",
    "jsx": "react",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
